<div class="event">
  <a href="#event-<%= item.event_id %>" class="event-created-at"><%= item.created_hm %></a>
  <span class="event-actor">
    <%= link_to item.user_name, member_path(item.user_id) %>
  </span>
  <span class="event-action">创建了项目:  </span>
  <span class="event-text"><%= link_to item.s_proj_name, team_project_path(current_team.id, item.s_proj_id) %></span>
</div>
